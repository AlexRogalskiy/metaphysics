{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2017"],
    "allowJs": true,
    "checkJs": false,
    "outDir": "./build",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true
  },
  "include": ["lib/**/*.js"]
}
