{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "jasmine": true,
    "es6": true
  },
  "extends": "react-app",
  "parser": "babel-eslint",
  "rules": {
    "import/first": 0,
    "no-useless-escape": 0,
    "no-empty-pattern": 0
  },  
  "globals": {
    "expect": false,
    "schema": false,
    "sinon": false,
    "expectPromiseRejectionToMatch": false
  }
}
